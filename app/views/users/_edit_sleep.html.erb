<h1>睡眠情報のBody部分です</h1>
<div>
  <%= form_with model: @health do |f| %>
  <form>
    <div class="form-group">
      <%= f.label :bedtime_at %>
      <%= f.time_field :bedtime_at %>
    </div>
    <div class="form-group">
      <%= f.label :getuptime_at %>
      <%= f.time_field :getuptime_at %>
    </div>
    <div class="form-group">
      <%= f.submit "登録", class: "btn-flat-border w-50" %>
      <button type="button" class="bbtn-cancel-border" data-dismiss="modal">
        キャンセル
      </button>
    </div>
  </form>
  <% end %>
</div>